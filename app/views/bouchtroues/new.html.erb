<div class = "container">

<h1 class = "container">Creer un profil de Bouchtroue</h1>

<%= simple_form_for @bouchtroue do |f| %>
  <div class="form-inputs">
    <%= f.input :name_profile %>
    <%= f.input :specialty %>
    <%= f.input :description %>
    <%= f.input :status %>
    <%= f.input :location %>
  </div>

  <div class="form-actions mb-5">
    <%= f.button :submit, class: "btn btn-primary" %>
  </div>
<% end %>

</div>
